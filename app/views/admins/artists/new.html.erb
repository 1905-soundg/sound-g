<h2>アーティスト・ジャンル・レーベル登録</h2>

<%= form_for @artist, url:admins_artists_path do |f| %>

    <h4>アーティスト名</h4>
    <%= f.text_field :artist %>

    <%= f.submit '登録' %>
<% end %>

<%= form_for @label, url: admins_labels_path do |f| %>

    <h4>レーベル名</h4>
    <%= f.text_field :label %>

    <%= f.submit '登録' %>
<% end %>

<%= form_for @genre, url:admins_genres_path do |f| %>

    <h4>ジャンル名</h4>
    <%= f.text_field :genre %>

    <%= f.submit '登録' %>
<% end %>