<div class="container">
	<h3><strong>顧客一覧</strong></h3>
	<body>
	<table class="table table-bordered">
		<thead>
			<tr>
				<th>名前</th>
				<th>電話番号</th>
				<th>メール</th>
				<th>会員情報</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= link_to user.last_name + user.first_name , admins_user_path(user.id), :style=>"color:#777;text-decoration: none;" %></td>
				<td><%= user.telephone_number %></td>
				<td><%= user.email %></td>
				<td>
					<% if user.deleted_at == nil %>
					<p>入会</p>
				    <% else %>
				    <p>退会</p>
				    <% end %>
				</td>
			</tr>
			<% end %>
		</tbody>
	</table>
</body>
</div>