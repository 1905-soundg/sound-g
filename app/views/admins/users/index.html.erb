<body>
	<table>
		<thead>
			<tr>
				<th>name</th>
				<th>tell</th>
				<th>mail</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= user.last_name + user.first_name %></td>
				<td><%= user.telephone_number %></td>
				<td><%= user.email %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</body>