<div class="container">
	<h3><strong>顧客一覧</strong></h3>
	<body>
	<table class="table table-bordered">
		<thead>
			<tr>
				<th>name</th>
				<th>tell</th>
				<th>mail</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= link_to user.last_name + user.first_name , admins_user_path(user.id), :style=>"color:#777;text-decoration: none;" %></td>
				<td><%= user.telephone_number %></td>
				<td><%= user.email %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</body>
</div>